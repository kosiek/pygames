{
    "python.analysis.typeCheckingMode": "strict",
    "editor.rulers": [
        {
            "column": 120,
            "color": "#ff0000"
        },
        {
            "column": 100,
            "color": "#ffa500"
        },
    ],
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "python.analysis.extraPaths": [
        "${workspaceFolder}/pywar/src"
    ],
}
